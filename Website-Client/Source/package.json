{
  "name": "es6-webpack2-starter",
  "version": "3.0.0",
  "description": "A template project for es6/7, webpack2/3, sass and postcss",
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.config.js --watch",
    "debug": "webpack --config webpack/dev.config.js --progress --display-error-details --color",
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --color",
    "continuous": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --color --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micooz/es6-webpack-starter.git"
  },
  "keywords": [
    "es6",
    "es7",
    "webpack2",
    "webpack3",
    "starter",
    "template",
    "postcss",
    "sass",
    "css module"
  ],
  "author": "Micooz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micooz/es6-webpack-starter/issues"
  },
  "homepage": "https://github.com/micooz/es6-webpack-starter#readme",
  "dependencies": {
    "@types/bytebuffer": "^5.0.37",
    "@types/websocket": "0.0.38",
    "bytebuffer": "^5.0.1",
    "font-awesome": "^4.7.0",
    "highlight.js": "^9.12.0",
    "quill": "^1.3.4",
    "quill-delta": "^3.6.2",
    "websocket": "^1.0.25"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "autoprefixer": "^7.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^3.3.0",
    "typescript": "^2.6.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  }
}
